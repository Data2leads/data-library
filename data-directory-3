    <!-- JavaScript Code -->
    <script>
        let selectedLeadType = '';

        function searchLeads() {
            const inputElement = document.getElementById('leadType');
            const input = inputElement.value.trim().toLowerCase();
            const table = document.getElementById('leadTable');
            const noResults = document.getElementById('noResults');
            const rows = table.querySelectorAll('tr:not(:first-child)');
            let matchCount = 0;
            let hasMatches = false;

            rows.forEach(row => {
                const rowText = row.textContent.toLowerCase();
                if (rowText.includes(input) && input !== '') {
                    if (matchCount < 100) { // Display up to 100 matches
                        row.style.display = '';
                        matchCount++;
                        hasMatches = true;
                    } else {
                        row.style.display = 'none';
                    }
                } else if (input === '') {
                    row.style.display = 'none';
                } else {
                    row.style.display = 'none';
                }
            });

            if (hasMatches && input !== '') {
                table.style.display = 'table';
                noResults.style.display = 'none';
            } else if (input !== '') {
                table.style.display = 'none';
                noResults.style.display = 'block';
            } else {
                table.style.display = 'none';
                noResults.style.display = 'none';
            }
        }

        // Add click event to table rows
        document.addEventListener('DOMContentLoaded', () => {
            const table = document.getElementById('leadTable');
            const tableRows = table.querySelectorAll('tr:not(:first-child)');
            tableRows.forEach(row => {
                row.addEventListener('click', function() {
                    // Remove highlight from other rows
                    document.querySelectorAll('.selected-row').forEach(r => r.classList.remove('selected-row'));
                    // Add highlight to selected row
                    this.classList.add('selected-row');
                    // Update selected lead type (assuming it's in the first cell)
                    selectedLeadType = this.cells[0].textContent.trim();
                    // Update button text and display it
                    const button = document.getElementById('checkoutButton');
                    button.textContent = 'Checkout ' + selectedLeadType;
                    button.style.display = 'inline-block';
                    // Optionally, update the button's link or data attributes here
                });
            });
        });
    </script>
</body>
</html>
